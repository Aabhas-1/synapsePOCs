						"source": [
							"#### 9. How to Drop rows with NA values specific to a particular column?"
						]
					},
					{
						"cell_type": "code",
						"metadata": {
							"jupyter": {
								"source_hidden": false,
								"outputs_hidden": false
							},
							"nteract": {
								"transient": {
									"deleting": false
								}
							},
							"collapsed": false
						},
						"source": [
							"#####################################################\r\n",
							"\r\n",
							"# Assuming df is your DataFrame\r\n",
							"df = spark.createDataFrame([\r\n",
							"(\"A\", 1, None),\r\n",
							"(\"B\", None, \"123\" ),\r\n",
							"(\"B\", 3, \"456\"),\r\n",
							"(\"D\", None, None),\r\n",
							"], [\"Name\", \"Value\", \"id\"])\r\n",
							"\r\n",
							"df.show()\r\n",
							"\r\n",
							"#####################################################\r\n",
							"\r\n",
							"df_2 = df.dropna(subset=['value'])\r\n",
							"df_2.show()"
						],
						"outputs": [],
						"execution_count": 25
					},
					{
						"cell_type": "markdown",
